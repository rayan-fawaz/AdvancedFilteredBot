entrypoint = "main.py"
modules = ["python-3.11"]

[nix]
channel = "stable-24_05"

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["sh", "-c", "python main.py"]
deploymentTarget = "gce"
ignorePorts = true

[workflows]
runButton = "Run Bot"

[[workflows.workflow]]
name = "Run Bot"
author = 39229138

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python src/main.py"

[[ports]]
localPort = 8080
externalPort = 80
